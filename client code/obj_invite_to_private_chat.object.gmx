    {
        num = get_integer("Write a numer of users for a private chat.", 1);
        for (var i = 0; i &lt; num; ++i)
        {
            name = get_string("Write a username to invite to a private chat.", "");
            if (name != "")
            {
                if (find_user(name))
                {
                    obj_network.for_user[obj_network.invite] = name;
                    obj_network.invite++;
                }
                else
                    show_message("User named " + name + " does not exist.");
